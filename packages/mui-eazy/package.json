{
  "name": "mui-eazy",
  "main": "dist/mui-eazy.es.js",
  "types": "dist/types/index.d.ts",
  "version": "0.0.10",
  "description": "Vite & TypeScript",
  "author": "闲D阿强",
  "files": [
    "dist"
